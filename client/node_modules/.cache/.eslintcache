);\n\n  useEffect(()=>{\n    if((sort===\"newest\")){\n      setfilteredProducts((prev) => \n      [...prev].sort((a,b)=>a.createdAt - b.createdAt))\n    } else if ((sort ===\"asc\")){\n      setfilteredProducts((prev) => \n      [...prev].sort((a,b)=>a.price - b.price))\n    } else {\n      setfilteredProducts((prev) => \n      [...prev].sort((a,b)=>b.price - a.price))\n    }\n  },[sort]);\n\n  return (\n    <Container>\n        {cat \n        ? filteredProducts.map((item)=>(\n            <Product item={item} key={item.id}/>\n        ))\n      : products.slice(0,8).map((item)=>(\n        <Product item={item} key={item.id}/>\n    ))}\n    </Container>\n  )\n}\n\nexport default Products","/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/components/Slider.jsx",[],"/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/data.js",[],"/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/components/CategoryItem.jsx",[],"/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/components/Product.jsx",[],"/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/requestMethods.js",[],"/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/redux/store.js",[],"/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/redux/cartRedux.js",[],"/Users/sanjayyadav/Documents/learnings/hands-on/e-commerce/client/src/pages/Success.jsx",[],{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":10,"column":8,"nodeType":"103","messageId":"104","endLine":10,"endColumn":13},{"ruleId":"101","severity":1,"message":"105","line":4,"column":10,"nodeType":"103","messageId":"104","endLine":4,"endColumn":25},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],"no-unused-vars","'axios' is defined but never used.","Identifier","unusedVar","'popularProducts' is defined but never used.","no-global-assign","no-unsafe-negation"]